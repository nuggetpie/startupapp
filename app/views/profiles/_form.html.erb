<%= simple_form_for(@profile) do |f| %>
  <%= f.error_notification %>

  <div class='row'>
    
    
    <div class="col-md-8">
      <div class="row">
        
        <div class="col-md-6">
          <div class="form-group">
           <%= f.input :first_name, input_html: {class: 'form-control'} %>
          </div>
        </div>
      
        <div class="col-md-6">
          <div class="form-group">
           <%= f.input :last_name, input_html: {class: 'form-control'} %>
          </div>
        </div>
      </div><!--closing col-md-8 -->

      <div class="row">
        <div class="col-md-12">
        
        <div class="form-group">
         <%= f.input :gender, input_html: {class: 'form-control'} %>
        </div>
        
        <div class="form-group">
          <%= f.input :interest, input_html: {class: 'form-control'} %>
        </div>
      </div>
    </div><!--closing row -->
  </div>
  </div> 
   
  <div class="form-group">
    <%= f.input :dob, start_year: Date.today.year - 110, end_year: Date.today.year - 8 %>
  </div>
  
  
  
  

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
